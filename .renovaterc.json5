{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    "github>guilhermewolf/atoca.house//.renovate/autoMerge.json5",
    "github>guilhermewolf/atoca.house//.renovate/changelogs.json5",
    "github>guilhermewolf/atoca.house//.renovate/customManagers.json5",
    "github>guilhermewolf/atoca.house//.renovate/grafanaDashboards.json5",
    "github>guilhermewolf/atoca.house//.renovate/groups.json5",
    "github>guilhermewolf/atoca.house//.renovate/overrides.json5",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(Europe/Amsterdam)"
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: [
    "prEditedNotification",
    "prIgnoreNotification"
  ],
  ignorePaths: [
    "**/resources/**"
  ],
  kubernetes: {
    managerFilePatterns: [
      "/\\.yaml(?:\\.j2)?$/"
    ],
  },
  "docker-compose": {
    managerFilePatterns: [
      "/(^|/)stacks/.+\\.ya?ml$/"
    ]
  },
  "helm-values": {
    managerFilePatterns: [
      "/(^|/)infra/k8s/.+/values\\.ya?ml$/",
      "/(^|/)apps/.+/values\\.ya?ml$/"
    ]
  },
}
